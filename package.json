    "crypto": "0.0.3",
    "fs": "0.0.1-security",
    "keccak": "^1.2.0",
    "promise": "^7.1.1",
    "properties": "^1.2.1",
    "randomstring": "^1.1.5",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "web3": "^0.18.2"
  },
  "devDependencies": {
    "tape": "^4.6.3",
    "tap": "10.3.0",
    "tap-parser": "~3.0.4",
    "jsdoc": "^3.4.3",
    "xo": "0.17.1",
    "browserify": "13.3.0",
    "tape-run": "3.0.0",
    "faucet": "0.0.1"
  },
  "scripts": {
    "test": "tap test/*.js && xo --ignore ./out/* && browserify test/*.js | tape-run",
