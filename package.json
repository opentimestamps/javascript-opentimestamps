{
  "name": "javascript-opentimestamps",
  "version": "0.2.0",
  "description": "JS Porting of OpenTimestamps",
  "main": "open-timestamps.js",
  "dependencies": {
    "bitcore-lib": "^0.13.7",
    "bitcore-message": "^1.0.4",
    "bytebuffer": "^5.0.1",
